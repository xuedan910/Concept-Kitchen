@media screen and (max-width: 500px) {
	.slideshow ul li figure iframe
	{
		height:35%;
		margin: 0;
		display: block;
	}
	.slideshow li{
		width:80%;
	}
	.slideshow nav span.nav-prev, .slideshow nav span.nav-next {
		top: 75%;
		-webkit-transform: translateY(-50%);
		transform: translateY(-50%);
	}
}
